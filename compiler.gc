method block() {
    if (accept("lbrace")) then {
        next()
        var ident1
        var s := sym.sym
        var tmp
        var params := []
        var body := []
        var havearrow := true
        var found := false
        var i := 0
        var toks := tokens
        // TODO This is a massive hack.
        for (0..15) each { i->
            if (i < (toks.size-1)) then {
                var t := toks.at((toks.size - i) - 1)
                if ((t.type /= "identifier") & (t.type /= "comma")
                    & (t.type /= "arrow") & found.not) then {
                    havearrow := false
                }
                if (t.type == "arrow") then {
                    found := true
                }
