    auto_count := auto_count + 1
    var applymeth := astmethod(astidentifier("apply", false),
        o.params, o.body, false)
    applymeth.selfclosure := true
    var objbody := astobject([applymeth])
    var obj := compilenode(objbody)
    o.register := obj
}
method compilefor(o) {
