if (runmode == "make") then {
    outfile.close()
    var cmd := "llvm-as -o " ++ modname ++ ".gco " ++ modname ++ ".ll"
    io.system(cmd)
    log_verbose("linking.")
    cmd := "llvm-link -o " ++ modname ++ ".bc gracelib.o "
    cmd := cmd ++ modname ++ ".gco"
    for (linkfiles) each { fn ->
        cmd := cmd ++ " " ++ fn
    }
    io.system(cmd)
    if (buildtype == "native") then {
        log_verbose("compiling to native.")
        cmd := "llvm-ld -o " ++ modname ++ " -native " ++ modname ++ ".bc"
        io.system(cmd)
    }
    if (buildtype == "run") then {
        cmd := "lli ./" ++ modname ++ ".bc"
        io.system(cmd)
    }
