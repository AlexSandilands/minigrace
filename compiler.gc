method next() {
    if (tokens.size > 0) then {
        if (sym.sym.kind /= "line") then {
            lastline := linenum
        }
        sym.sym := tokens.pop()
        if ((sym.sym.kind == "line") & (tokens.size > 0)) then {
            var l := sym.sym.value
            if l /= linenum then {
                linenum := l
                next()
            }
        }
    } else {
        sym.sym := object { var kind := "eof" }
    }
}

